#. type: Plain text
#: hostid.1:5
msgid "hostid - print the numeric identifier for the current host"
msgstr "hostid - 打印当前主机的数字标识符"

#. type: SH
#: hostid.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: hostid.1:8
msgid "B<hostid> [I<OPTION>]"
msgstr "B<hostid> [I<选项>]"

#. type: SH
#: hostid.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: hostid.1:12
msgid "Print the numeric identifier (in hexadecimal) for the current host."
msgstr "打印当前主机的数字标识符 （十六进制）。"

#. type: TP
#: hostid.1:12
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: hostid.1:15
msgid "display this help and exit"
msgstr "显示帮助然后退出"

#. type: TP
#: hostid.1:15
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: hostid.1:18
msgid "output version information and exit"
msgstr "输出版本信息然后退出"

#. type: SH
#: hostid.1:18
#, no-wrap
