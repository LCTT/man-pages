#. type: TH
#: shuf.1:2
#, no-wrap
msgid "SHUF"
msgstr "SHUF"

#. type: TH
#: shuf.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: shuf.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: shuf.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: shuf.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: shuf.1:5
msgid "shuf - generate random permutations"
msgstr "shuf - 生成随机序列"

#. type: SH
#: shuf.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: shuf.1:8
msgid "B<shuf> [I<OPTION>]... [I<FILE>]"
msgstr "B<shuf> [I<选项>]... [I<文件>]"

#. type: Plain text
#: shuf.1:11
msgid "B<shuf> I<-e >[I<OPTION>]... [I<ARG>]..."
msgstr "B<shuf> I<-e >[I<选项>]... [I<参数>]..."

#. type: Plain text
#: shuf.1:14
msgid "B<shuf> I<-i LO-HI >[I<OPTION>]..."
msgstr "B<shuf> I<-i LO-HI >[I<选项>]..."

#. type: SH
#: shuf.1:14
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: shuf.1:18
msgid "Write a random permutation of the input lines to standard output."
msgstr "向标准输出打印输入行的随机序列"

#. type: Plain text
#: shuf.1:20
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr "长选项必须用的参数在短选项中也是必须的。"

#. type: TP
#: shuf.1:20
#, no-wrap
msgid "B<-e>, B<--echo>"
msgstr "B<-e>, B<--echo>"

#. type: Plain text
#: shuf.1:23
msgid "treat each ARG as an input line"
msgstr "每一个参数都作为输入行"

#. type: TP
#: shuf.1:23
#, no-wrap
msgid "B<-i>, B<--input-range>=I<LO-HI>"
msgstr "B<-i>, B<--input-range>=I<LO-HI>"

#. type: Plain text
#: shuf.1:26
msgid "treat each number LO through HI as an input line"
msgstr "从 LO 到 HI 的每一个数都作为输入行"

#. type: TP
#: shuf.1:26
#, no-wrap
msgid "B<-n>, B<--head-count>=I<COUNT>"
msgstr "B<-n>, B<--head-count>=I<数目>"

#. type: Plain text
#: shuf.1:29
msgid "output at most COUNT lines"
msgstr "输出行数最多为指定的数目"

#. type: TP
#: shuf.1:29
#, no-wrap
msgid "B<-o>, B<--output>=I<FILE>"
msgstr "B<-o>, B<--output>=I<文件>"

#. type: Plain text
#: shuf.1:32
msgid "write result to FILE instead of standard output"
msgstr "把结果写入指定的文件中"

#. type: TP
#: shuf.1:32
#, no-wrap
msgid "B<--random-source>=I<FILE>"
msgstr "B<--random-source>=I<文件>"

#. type: Plain text
#: shuf.1:35
msgid "get random bytes from FILE"
msgstr "从指定的文件中获取随机字节"

#. type: TP
#: shuf.1:35
#, no-wrap
msgid "B<-z>, B<--zero-terminated>"
msgstr "B<-z>, B<--zero-terminated>"

#. type: Plain text
#: shuf.1:38
msgid "end lines with 0 byte, not newline"
msgstr "以字节 0 作为行结束符"

#. type: TP
#: shuf.1:38
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: shuf.1:41
msgid "display this help and exit"
msgstr "显示帮助然后退出"

#. type: TP
#: shuf.1:41
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: shuf.1:44
msgid "output version information and exit"
msgstr "输出版本信息然后退出"

#. type: Plain text
#: shuf.1:46
msgid "With no FILE, or when FILE is -, read standard input."
msgstr "当参数中没有文件， 或者文件为 - ， 将从标准输入中读取。 "

#. type: SH
#: shuf.1:46
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: shuf.1:48
msgid "Written by Paul Eggert."
msgstr "由 Paul Eggert 撰写。"

#. type: SH
#: shuf.1:48
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: shuf.1:50
msgid "Report shuf bugs to bug-coreutils@gnu.org"
msgstr "发送 shuf 的错误信息到： bug-coreutils@gnu.org"

#. type: Plain text
#: shuf.1:52
msgid ""
"GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: shuf.1:54
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: shuf.1:56
msgid ""
"Report shuf translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送 shuf 的翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

#. type: SH
#: shuf.1:56
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权声明"

#. type: Plain text
#: shuf.1:59
msgid ""
"Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr ""
"Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
#: shuf.1:62
msgid ""
"This is free software: you are free to change and redistribute it.  There is "
"NO WARRANTY, to the extent permitted by law."
msgstr ""
"这是一个自由软件： 你可以自由修改和重新发布它。 在法律允许的范围内， 不提供任"
"何担保。"

#. type: SH
#: shuf.1:62
#, no-wrap
msgid "SEE ALSO"
msgstr "请参阅"

#. type: Plain text
#: shuf.1:70
msgid ""
"The full documentation for B<shuf> is maintained as a Texinfo manual.  If "
"the B<info> and B<shuf> programs are properly installed at your site, the "
"command"
msgstr ""
"本程序的完整文档使用 Texinfo 手册进行维护。如果 B<info> 和本程序已经正确地安"
"装在了你的电脑上，通过下述命令"

#. type: Plain text
#: shuf.1:72
msgid "B<info coreutils \\(aqshuf invocation\\(aq>"
msgstr "B<info coreutils \\(aqshuf invocation\\(aq>"

#. type: Plain text
#: shuf.1:74
msgid "should give you access to the complete manual."
msgstr "可以访问完整的使用手册。"

