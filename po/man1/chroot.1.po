#. type: TH
#: chroot.1:2
#, no-wrap
msgid "CHROOT"
msgstr "CHROOT"

#. type: TH
#: chroot.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: chroot.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: chroot.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: chroot.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: chroot.1:5
msgid "chroot - run command or interactive shell with special root directory"
msgstr "chroot - 以指定的根目录运行命令或者交互式 shell"

#. type: SH
#: chroot.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: chroot.1:8
msgid "B<chroot> [I<OPTION>] I<NEWROOT >[I<COMMAND >[I<ARG>]...]"
msgstr "B<chroot> [I<选项>] I<新的根目录 >[I<命令 >[I<参数>]...]"

#. type: Plain text
#: chroot.1:11
msgid "B<chroot> I<OPTION>"
msgstr "B<chroot> I<选项>"

#. type: SH
#: chroot.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: chroot.1:15
msgid "Run COMMAND with root directory set to NEWROOT."
msgstr "以新的根目录运行命令"

#. type: TP
#: chroot.1:15
#, no-wrap
msgid "B<--userspec>=I<USER>:GROUP"
msgstr "B<--userspec>=I<用户>:组"

#. type: Plain text
#: chroot.1:18
msgid "specify user and group (ID or name) to use"
msgstr "使用指定的用户和组 （ID 或者名称）"

#. type: TP
#: chroot.1:18
#, no-wrap
msgid "B<--groups>=I<G_LIST>"
msgstr "B<--groups>=I<G_LIST>"

#. type: Plain text
#: chroot.1:21
msgid "specify supplementary groups as g1,g2,..,gN"
msgstr "指定附属组为 g1， g2， .. ， gN"

#. type: TP
#: chroot.1:21
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: chroot.1:24
msgid "display this help and exit"
msgstr "显示帮助然后退出"

#. type: TP
#: chroot.1:24
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: chroot.1:27
msgid "output version information and exit"
msgstr "输出版本信息然后退出"

#. type: Plain text
#: chroot.1:29
msgid ""
"If no command is given, run '${SHELL} B<-i>' (default: '/bin/sh B<-i>')."
msgstr "如果没有指定命令， 运行 '${SHELL} B<-i>' （ 默认： '/bin/sh B<-i>'）。"

#. type: SH
#: chroot.1:29
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: chroot.1:31
msgid "Written by Roland McGrath."
msgstr "由 Roland McGrath 撰写。"

#. type: SH
#: chroot.1:31
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: chroot.1:33
msgid "Report chroot bugs to bug-coreutils@gnu.org"
msgstr "发送 chroot 错误信息到： bug-coreutils@gnu.org"

#. type: Plain text
#: chroot.1:35
msgid ""
"GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: chroot.1:37
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: chroot.1:39
msgid ""
"Report chroot translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送 chroot 翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

