#. type: TH
#: mkfifo.1:2
#, no-wrap
msgid "MKFIFO"
msgstr "MKFIFO"

#. type: TH
#: mkfifo.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: mkfifo.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: mkfifo.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: mkfifo.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: mkfifo.1:5
msgid "mkfifo - make FIFOs (named pipes)"
msgstr "mkfifo - 创建 FIFOs （命名管道）"

#. type: SH
#: mkfifo.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: mkfifo.1:8
msgid "B<mkfifo> [I<OPTION>]... I<NAME>..."
msgstr "B<mkfifo> [I<选项>]... I<名称>..."

#. type: SH
#: mkfifo.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: mkfifo.1:12
msgid "Create named pipes (FIFOs) with the given NAMEs."
msgstr "用指定的文件名创建命名管道（FIFOs）"

#. type: Plain text
#: mkfifo.1:14
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr "长选项必须用的参数在短选项中也是必须的。"

#. type: TP
#: mkfifo.1:14
#, no-wrap
msgid "B<-m>, B<--mode>=I<MODE>"
msgstr "B<-m>, B<--mode>=I<模式>"

#. type: Plain text
#: mkfifo.1:17
msgid "set file permission bits to MODE, not a=rw - umask"
msgstr "设置文件的权限位为MODE，不是a=rw - umask"

#. type: TP
#: mkfifo.1:17
#, no-wrap
msgid "B<-Z>, B<--context>=I<CTX>"
msgstr "B<-Z>, B<--context>=I<CTX>"

#. type: Plain text
#: mkfifo.1:20
msgid "set the SELinux security context of each NAME to CTX"
msgstr "设置每个CTX名称的SELinux的安全上下文"

#. type: TP
#: mkfifo.1:20
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: mkfifo.1:23
msgid "display this help and exit"
msgstr "显示帮助并退出"

#. type: TP
#: mkfifo.1:23
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: mkfifo.1:26
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#. type: SH
#: mkfifo.1:26
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: mkfifo.1:28
msgid "Written by David MacKenzie."
msgstr "由David MacKenzie撰写"

#. type: SH
#: mkfifo.1:28
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: mkfifo.1:30
msgid "Report mkfifo bugs to bug-coreutils@gnu.org"
msgstr "发送 mkfifo 的错误信息到： bug-coreutils@gnu.org"

#. type: Plain text
#: mkfifo.1:32
msgid ""
"GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: mkfifo.1:34
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: mkfifo.1:36
msgid ""
"Report mkfifo translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送 mkfifo 的翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

